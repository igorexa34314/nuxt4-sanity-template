{
  "tabWidth": 2,
  "singleQuote": true,
  "endOfLine": "lf",
  "useTabs": false,
  "printWidth": 80,
  "semi": true,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "singleAttributePerLine": false,
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindConfig": "./app/assets/css/main.css",
  "tailwindAttributes": [
    "class",
    "className",
    "class:list",
    "enter-to-class",
    "enter-from-class",
    "leave-to-class",
    "leave-from-class",
    "enter-active-class",
    "leave-active-class"
  ],
  "tailwindFunctions": ["tw", "cn"]
}
